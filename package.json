{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:edmilson-dk/inn-inteligence-test-api.git",
  "author": "edmilson-dk <droidvidaboa@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "npm run knex:migrate:latest && npm run build && NODE_PATH=./build node build/src/main/server.js",
    "dev": "NODE_PATH=./ npx ts-node-dev --respawn --pretty --transpile-only src/main/server.ts",
    "knex:migrate:make": "knex --knexfile knexfile.ts -x ts migrate:make",
    "knex:migrate:latest": "knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback --all"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/knex": "^0.16.1",
    "@types/node": "^15.6.1",
    "@types/pg": "^8.6.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "knex": "^0.95.6",
    "pg": "^8.6.0",
    "ts-node": "^10.0.0"
  }
}
